<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <parameters>
    <parameter key="knp_dictionary.data_collector.dictionary_data_collector.class">Knp\DictionaryBundle\DataCollector\DictionaryDataCollector</parameter>
    <parameter key="knp_dictionary.data_collector.dictionary_data_collector.template">KnpDictionaryBundle::layout.html.twig</parameter>
    <parameter key="knp_dictionary.dictionary.dictionary.class">Knp\DictionaryBundle\Dictionary\Dictionary</parameter>
    <parameter key="knp_dictionary.dictionary.dictionary_registry.class">Knp\DictionaryBundle\Dictionary\DictionaryRegistry</parameter>
    <parameter key="knp_dictionary.dictionary.value_transformer.constant_transformer.class">Knp\DictionaryBundle\Dictionary\ValueTransformer\ConstantTransformer</parameter>
    <parameter key="knp_dictionary.form.type.dictionary_type.class">Knp\DictionaryBundle\Form\Type\DictionaryType</parameter>
    <parameter key="knp_dictionary.validator.constraints.dictionary_validator.class">Knp\DictionaryBundle\Validator\Constraints\DictionaryValidator</parameter>
    <parameter key="knp_dictionary.twig.dictionary_extension.class">Knp\DictionaryBundle\Twig\DictionaryExtension</parameter>
    <parameter key="knp_dictionary.data_fixtures.dictionary_provider.class">Knp\DictionaryBundle\DataFixtures\DictionaryProvider</parameter>
  </parameters>

  <services>
    <service id="knp_dictionary.data_collector.dictionary_data_collector" class="%knp_dictionary.data_collector.dictionary_data_collector.class%">
      <tag name="data_collector" template="%knp_dictionary.data_collector.dictionary_data_collector.template%" id="dictionary"/>
      <argument type="service" id="knp_dictionary.registry"/>
    </service>

    <service id="knp_dictionary.dictionary.dictionary_factory" class="Knp\DictionaryBundle\Dictionary\DictionaryFactory"/>

    <service id="knp_dictionary.dictionary.dictionary_registry" class="%knp_dictionary.dictionary.dictionary_registry.class%"/>

    <service id="knp_dictionary.dictionary.value_transformer.constant_transformer" class="%knp_dictionary.dictionary.value_transformer.constant_transformer.class%">
      <tag name="knp_dictionary.value_transformer"/>
    </service>

    <service id="knp_dictionary.form.type.dictionary_type" class="%knp_dictionary.form.type.dictionary_type.class%">
      <tag name="form.type" alias="dictionary"/>
      <argument type="service" id="knp_dictionary.registry"/>
    </service>

    <service id="knp_dictionary.validator.constraints.dictionary_validator" class="%knp_dictionary.validator.constraints.dictionary_validator.class%">
      <tag name="validator.constraint_validator" alias="knp_dictionary.dictionary_validator"/>
      <argument type="service" id="knp_dictionary.registry"/>
    </service>

    <service id="knp_dictionary.twig.dictionary_extension" class="%knp_dictionary.twig.dictionary_extension.class%">
      <tag name="twig.extension"/>
      <argument type="service" id="knp_dictionary.registry"/>
    </service>

    <service id="knp_dictionary.data_fixtures.dictionary_provider" class="%knp_dictionary.data_fixtures.dictionary_provider.class%">
      <argument type="service" id="knp_dictionary.registry"/>
    </service>

    <service id="knp_dictionary.registry" alias="knp_dictionary.dictionary.dictionary_registry"/>
  </services>
</container>
