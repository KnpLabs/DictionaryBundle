<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Knp\DictionaryBundle\DictionaryCollection" public="true"/>

    <service id="Knp\DictionaryBundle\Dictionary\Factory\CallableFactory" public="false">
        <argument type="service" id="Symfony\Component\DependencyInjection\ContainerInterface"/>
        <tag name="knp_dictionary.factory"/>
    </service>

    <service id="Knp\DictionaryBundle\Faker\Provider\Dictionary" public="false">
      <argument type="service" id="Knp\DictionaryBundle\DictionaryCollection"/>
    </service>

    <service id="Knp\DictionaryBundle\Twig\DictionaryExtension" public="false">
      <argument type="service" id="Knp\DictionaryBundle\DictionaryCollection"/>
      <tag name="twig.extension"/>
    </service>

    <service id="Knp\DictionaryBundle\Validator\Constraints\DictionaryValidator" public="false">
      <argument type="service" id="Knp\DictionaryBundle\DictionaryCollection"/>
      <tag name="validator.constraint_validator" alias="knp_dictionary.dictionary_validator"/>
    </service>
  </services>
</container>
