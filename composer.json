{
    "name": "knplabs/dictionary-bundle",
    "type": "symfony-bundle",
    "description": "Are you often tired to repeat static choices like gender or civility in your apps ?",
    "keywords": [
        "dictionary"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Knplabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Shivoham",
            "homepage": "https://github.com/Shivoham"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/config": "~3.4 || ~4.2 || ~5.0",
        "symfony/dependency-injection": "~3.4 || ~4.2 || ~5.0",
        "symfony/form": "~3.4 || ~4.2 || ~5.0",
        "symfony/http-foundation": "~3.4 || ~4.2 || ~5.0",
        "symfony/http-kernel": "~3.4 || ~4.2 || ~5.0",
        "symfony/validator": "~3.4 || ~4.2 || ~5.0",
        "twig/twig": "~2.4 || ~3.0"
    },
    "require-dev": {
        "beberlei/assert": "^3.0",
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "fzaninotto/faker": "~1.3",
        "pedrotroller/php-cs-custom-fixer": "^2.19",
        "pedrotroller/symfony-integration-checker": "^2.0",
        "phpspec/phpspec": "~6.0",
        "sebastian/comparator": "^1.2.4"
    },
    "suggest": {
        "knplabs/rad-fixtures-load": "Allows to autoregister the dictionary provider as an alice provider"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Knp\\DictionaryBundle\\": "src/Knp/DictionaryBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Knp\\": "spec/Knp",
            "spec\\PHPSpec\\": "spec/PHPSpec"
        }
    },
    "minimum-stability": "stable"
}
