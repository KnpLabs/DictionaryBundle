{
    "name":         "knplabs/dictionary-bundle",
    "description":  "Are you often tired to repeat static choices like gender or civility in your apps ?",
    "keywords":     ["dictionary"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name": "Knplabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Shivoham",
            "homepage": "https://github.com/Shivoham"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "symfony/http-kernel": "~3.3",
        "symfony/dependency-injection": "~3.3",
        "symfony/http-foundation": "~3.3",
        "symfony/config": "~3.3",
        "symfony/form": "~3.3",
        "symfony/validator": "~3.3",
        "twig/twig": "^1.12"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.5",
        "bossa/phpspec2-expect": "~1.0",
        "fzaninotto/faker": "^1.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Knp\\DictionaryBundle\\": "./src" }
    },
    "autoload-dev": {
        "psr-0": {
            "spec": "./spec"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    },
    "suggest": {
        "fzaninotto/faker": "To use the dictionary provider",
        "knplabs/rad-fixtures-load": "Allows to autoregister the dictionary provider as an alice provider"
    }
}
