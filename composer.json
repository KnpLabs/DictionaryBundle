{
    "name":         "knplabs/dictionary-bundle",
    "description":  "Are you often tired to repeat static choices like gender or civility in your apps ?",
    "keywords":     ["dictionary"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name": "Knplabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Shivoham",
            "homepage": "https://github.com/Shivoham"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "symfony/config": "~3.0 || ~4.0",
        "symfony/dependency-injection": "~3.0 || ~4.0",
        "symfony/form": "~3.0 || ~4.0",
        "symfony/http-foundation": "~3.0 || ~4.0",
        "symfony/http-kernel": "~3.0 || ~4.0",
        "symfony/validator": "~3.0 || ~4.0",
        "twig/twig": "~1.12 || ~2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.3",
        "leanphp/phpspec-code-coverage": "^4.0",
        "pedrotroller/php-cs-custom-fixer": "^2.6",
        "pedrotroller/symfony-integration-checker": "~2.0.0",
        "phpspec/phpspec": "~4.0",
        "phpspec/prophecy": ">=1.7.2",
        "sebastian/comparator": "~1.2.4",
        "webmozart/assert": "^1.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Knp\\DictionaryBundle\\": "src/Knp/DictionaryBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "suggest": {
        "knplabs/rad-fixtures-load": "Allows to autoregister the dictionary provider as an alice provider"
    }
}
